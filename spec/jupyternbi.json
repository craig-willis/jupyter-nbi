{
  "key": "jupyternbi",
  "label": "Jupyter for NBI",
  "description": "Jupyter environment for National Bridge Infrastructure (NBI) data access",
  "image": {
    "registry": "",
    "name": "craigwillis/jupyter-nbi",
    "tags": [
      "latest"
    ]
  },
  "display": "stack",
  "access": "external",
  "config": [
    {
      "name": "PASSWORD",
      "label": "Password",
      "isPassword": true
    }
  ],
  "ports": [
    {
      "port": 8888,
      "protocol": "http",
      "contextPath": "/"
    }
  ],
  "readinessProbe": {
    "type": "http",
    "path": "/static/base/images/favicon.ico",
    "port": 8888,
    "initialDelay": 5,
    "timeout": 600
  },
  "volumeMounts": [
    {
      "mountPath": "/home/jovyan/work",
      "defaultPath":"/"
    }
  ],
  "resourceLimits": {
    "cpuMax": 2000,
    "cpuDefault": 100,
    "memMax": 8000,
    "memDefault": 50
  },
  "tags": [
    "7",
    "21",
    "20",
    "28",
    "40"
  ],
  "authRequired": true
}
